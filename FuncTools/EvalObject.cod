class inherited FuncObj "Evaluate применяемый к объекту";

inclass public

  func New synonym CreateEx(functext: String): EvalObject;
    Result = Create;
    Result.functext = functext;
  end;

inobject private
  var functext: String;

inobject public

  func Вызвать synonym Call(Params: Variant[]): Variant;
    var MyObj: Object;
    MyObj = Params[1] as Object;
    return MyObj.Evaluate(self.functext);
  end;
end